@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>FlughafentaxiAdmin</title>


    @*@Styles.Render("~/Content/css")*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/styles.css" rel="stylesheet" />
    @*@Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
    <!-- Header -->
    <div id="top-nav" class="navbar navbar-inverse navbar-static-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Flughafentaxibond Administration</a>
                <a href="/AdminPanel/AdminPanel">Statistic</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right">

                    <li class="dropdown">
                        <a class="dropdown-toggle" role="button" data-toggle="dropdown" href="#"><i class="glyphicon glyphicon-user"></i> Admin <span class="caret"></span></a>
                        <ul id="g-account-menu" class="dropdown-menu" role="menu">
                            <li><a href="#">My Profile</a></li>
                        </ul>
                    </li>
                    <li><a href="/Login/Logout"><i class="glyphicon glyphicon-lock"></i> Logout</a></li>
                </ul>
            </div>
        </div><!-- /container -->
    </div>
    <!-- /Header -->
    <!-- Main -->
    <div class="container-fluid">
        <div class="row" style="padding-left:10px; padding-right:10px;">
        
            <div class="col-lg-6">
                <h4 style="text-align:center;">FROM AIRPORT (nerealizovane)</h4>
                @if (ViewBag.from != null)
                {
                    <div class="list-group">
                       @foreach (var item in (List<TaxiWebSite.Controllers.FromAir>)ViewBag.from)
                        {
                        <a id="list_horror" style="cursor:pointer" class="list-group-item">
                            <h5 class="list-group-item-heading"><b>@item.datum</b></h5>
                            <p class="list-group-item-text">@(item.email + " [" + item.phone + "]")</p>
                            <p class="list-group-item-text"><b>@item.adresa</b></p>
                            <p class="list-group-item-text">@item.name</p>
                            <p class="list-group-item-text">@(item.price + "e")</p>
                        </a>
                            }
                    </div>                 
                }
            </div>
            <div class="col-lg-6">
                <h4 style="text-align:center;">TO AIRPORT (nerealizovane)</h4>
                @if (ViewBag.to != null)
                {
                    <div class="list-group">
                        @foreach (var item in (List<TaxiWebSite.Controllers.FromAir>)ViewBag.to)
                        {
                            <a id="list_horror" style="cursor:pointer" class="list-group-item">
                                <h5 class="list-group-item-heading"><b>@item.datum</b></h5>
                                <p class="list-group-item-text">@(item.email + " ["+item.phone+"]")</p>
                                <p class="list-group-item-text"><b>@item.adresa</b></p>
                                <p class="list-group-item-text">@item.name</p>
                                <p class="list-group-item-text">@(item.price + "e")</p>
                            </a>
                        }
                    </div>
                }
            </div>
            <hr>
        </div><!--/col-span-9-->
    </div>
    </div>
    <!-- /Main -->
    <footer class="text-center">Powered by DViG team <a href="http://www.dvig.rs"><strong>dvig.rs</strong></a></footer>



    <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src="~/Scripts/Chart.min.js"></script>


</body>
</html>