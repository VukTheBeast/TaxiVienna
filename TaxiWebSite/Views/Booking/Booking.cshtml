
@{
    ViewBag.Title = "Booking";
}
@Html.Hidden("InputZipCode2")



<style>
    .jumbotron{
        /*font-size:inherit;*/
    }
    .jumbotron #scrolingBooking {
        overflow-y: scroll; height:600px;
    }
    #container1 {
    background-color: #e2dada;
}
    .form-control {
        width:inherit;
    }
/*.centered-form {
    margin-top: 120px;
    margin-bottom: 120px;
}*/

.centered-form .panel {
    background: rgba(255, 255, 255, 0.8);
    box-shadow: rgba(0, 0, 0, 0.3) 20px 20px 20px;
}

