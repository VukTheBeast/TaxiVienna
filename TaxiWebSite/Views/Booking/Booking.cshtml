
@{
    ViewBag.Title = "Booking";
}
@Html.Hidden("InputZipCode2")



<style>
    .jumbotron{
        height:1200px;
        /*font-size:inherit;*/
    }
    /*.jumbotron #scrolingBooking {
        overflow-y: scroll; height:600px;
    }*/
    #container1 {
    background-color: #e2dada;
}
    .form-control {
        width:inherit;
    }
/*.centered-form {
    margin-top: 120px;
    margin-bottom: 120px;
}*/

.centered-form .panel {
    background: rgba(255, 255, 255, 0.8);
    box-shadow: rgba(0, 0, 0, 0.3) 20px 20px 20px;
}

.panel-body {
padding: 20px;
}

.input-sm{
    height:25px;
}

    select.input-sm {
        height:28px;
    }
    h3{
        color:black;
    }

    .ui-datepicker{
        z-index:9999;
    }



    
        .glyphicon-refresh-animate {
        -animation: spin .7s infinite linear;
        -webkit-animation: spin2 .7s infinite linear;
    }

    @@-webkit-keyframes spin2 {
        from {
            -webkit-transform: rotate(0deg);
        }

        to {
            -webkit-transform: rotate(360deg);
        }
    }

    @@keyframes spin {
        from {
            transform: scale(1) rotate(0deg);
        }

        to {
            transform: scale(1) rotate(360deg);
        }
    }
    
</style>



@*<h2>Booking</h2>*@

<div id="scrolingBooking" class="container">
    <div class="container" id="container1">
        <div class="row">
            @*<div class="col-xs-12 col-sm-8 col-md-4 col-sm-offset-2 col-md-offset-4">*@
                <div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title text-center">Booking To Airport</h3>
                    </div>
                    <div class="panel-body">
                        <form role="form" id="frm_Reservacija">
                            <table>
                                <tr>
                                    <td><label class="input-sm">Pick-up date</label></td>
                                    <td><input type="text" class="form-control input-sm datum" name="InputDate" id="InputDate" placeholder="Enter Date" required></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Pick up time</label></td>
                                    <td>
                                        <select class="input-sm" id="pickupHRS" name="pickupHRS">
                                        <option value="" selected="selected">Hrs</option>
                                        <option value="00">00</option>
                                        <option value="01">01</option>
                                        <option value="02">02</option>
                                        <option value="03">03</option>
                                        <option value="04">04</option>
                                        <option value="05">05</option>
                                        <option value="06">06</option>
                                        <option value="07">07</option>
                                        <option value="08">08</option>
                                        <option value="09">09</option>
                                        <option value="10">10</option>
                                        <option value="11">11</option>
                                        <option value="12">12</option>
                                        <option value="13">13</option>
                                        <option value="14">14</option>
                                        <option value="15">15</option>
                                        <option value="16">16</option>
                                        <option value="17">17</option>
                                        <option value="18">18</option>
                                        <option value="19">19</option>
                                        <option value="20">20</option>
                                        <option value="21">21</option>
                                        <option value="22">22</option>
                                        <option value="23">23</option>
                                    </select>
                                        <select class="input-sm" id="pickupMIN" name="pickupMIN">
                                            <option value="" selected="selected">Min</option>
                                            <option value="00">00</option>
                                            <option value="05">05</option>
                                            <option value="10">10</option>
                                            <option value="15">15</option>
                                            <option value="20">20</option>
                                            <option value="25">25</option>
                                            <option value="30">30</option>
                                            <option value="35">35</option>
                                            <option value="40">40</option>
                                            <option value="45">45</option>
                                            <option value="50">50</option>
                                            <option value="55">55</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">House</label></td>
                                    <td><input type="text" id="house" name="house" class="input-sm"/></td>
                                    <td><label class="input-sm">Floor</label></td>
                                    <td><input type="text" id="flor" name="flor" class="input-sm" /></td>
                                    </tr>
                                <tr>
                                    <td><label class="input-sm">Door</label></td>
                                    <td><input type="text" id="door" name="door" class="input-sm" /></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Pick up from:</label></td>
                                    <td>
                                        <select class="form-control input-sm" id="InputPickupFrom" name="InputPickupFrom">
                                            <option value="5" selected="selected">from home</option>
                                            <option value="6">from office</option>
                                            <option value="7">from train station</option>
                                            <option value="9">from hotel</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Your full name:</label></td>
                                    <td><input type="text" name="InputFullName" id="InputFullName" class="input-sm" placeholder="Enter Name"></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Location:</label></td>
                                    <td>@Html.DropDownList("InputLocation", (List<SelectListItem>)ViewBag.listaGradovi, new { @style = "width:150px", @class = "input-group input-sm" })</td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Zip code:</label></td>
                                    <td>@Html.DropDownList("InputZipCode", (List<SelectListItem>)ViewBag.listaOblastiWien, new { @style = "width:150px", @class = "input-group input-sm" })</td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Street:</label></td>
                                    <td><select class="input-group input-sm" id="street" name="street" style="width:150px">
                                        <option></option>
                                    </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Your phone number:</label></td>
                                    <td><input type="tel" class="input-sm" id="InputTelFirst" name="InputTelFirst" placeholder="Enter Phone" required></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Your email:</label></td>
                                    <td><input type="email" class="input-sm" id="InputEmail" name="InputEmail" placeholder="Enter Email" required></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Type of car:</label></td>
                                    <td><select id="car_type" name="car_type" class="input-group input-sm" label="Typ Auto:" placeholder="Enter type of Car" onchange="recalculate_price();">
                                            <option value="3">Grobram taxi (5 bis 8 personen)</option>
                                            <option value="2">Business klase (bis 4 personen)</option>
                                            <option value="1">Economy klase (bis 4 personen)</option>
                                        </select></td>
                                    <td><label class="input-sm">Suitcases:</label></td>
                                    <td><select id="suitcases" name="suitcases" class="input-group input-sm" label="Anzahl der Koffer:" placeholder="Enter Suitcases" onchange="recalculate_price();">
                                            <option value="0">No suitcases</option>
                                            <option value="1" selected="selected">1 Suitcase</option>
                                            <option value="2">2 Suitcase</option>
                                            <option value="3">3 Suitcase</option>
                                            <option value="4">4 Suitcase</option>
                                            <option value="5">5 Suitcase</option>
                                            <option value="6">6 Suitcase</option>
                                            <option value="7">7 Suitcase</option>
                                            <option value="8">8 Suitcase</option>
                                            <option value="999">More from 8 on request</option>
                                        </select></td>
                                    </tr>
                                <tr>
                                    <td><label class="input-sm">Hand luggage:</label></td>
                                    <td><select id="hand_package" name="hand_package" class="input-group input-sm" label="Handgepäck:" placeholder="Hand laggage" onchange="recalculate_price();">
                                            <option value="0">No hand luggage</option>
                                            <option value="1" selected="selected">1 Piece</option>
                                            <option value="2">2 Piece</option>
                                            <option value="3">3 Piece</option>
                                            <option value="4">4 Piece</option>
                                            <option value="5">5 Piece</option>
                                            <option value="6">6 Piece</option>
                                            <option value="7">7 Piece</option>
                                            <option value="8">8 Piece</option>
                                            <option value="999">More from 8 on request</option>
                                        </select></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm" for="form-group-input">Payment</label></td>
                                    <td><select id="payment" name="payment" class="input-group input-sm" label="Bezahlung:">
                                        <option value="cash">Cash</option>
                                        <option value="card">Credit Card</option>
                                        <option value="bank">Bank Transfer</option>
                                    </select></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Comment</label></td>
                                    <td><textarea id="comment" name="comment" rows="2" cols="25" class="input-group-sm"></textarea></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm">Price:</label></td>
                                    <td><label id="lblPrice">0 €</label></td>
                                </tr>
                                <tr>
                                    <td><label class="input-sm" for="return">Return trip</label></td>
                                    <td><input type="checkbox" id="return" name="return" /></td>
                                </tr>
                            </table>    
                            <div class="form-group input-group-sm" id="divTrip" style="display:none">
                                <label class="input-sm">Return date</label>
                                <input type="text" class="form-control input-sm datumReturn" name="ReturnDate" id="ReturnDate" placeholder="Enter Date" required>
                                <label class="input-sm">Return time</label>
                                <div class="row">
                                    <div class="col-md-3" style="width:100px">
                                        <div class="input-group">
                                            <select class="form-control input-sm" id="pickupHRSReturn" name="pickupHRSReturn">
                                                <option value="" selected="selected">Hrs</option>
                                                <option value="00">00</option>
                                                <option value="01">01</option>
                                                <option value="02">02</option>
                                                <option value="03">03</option>
                                                <option value="04">04</option>
                                                <option value="05">05</option>
                                                <option value="06">06</option>
                                                <option value="07">07</option>
                                                <option value="08">08</option>
                                                <option value="09">09</option>
                                                <option value="10">10</option>
                                                <option value="11">11</option>
                                                <option value="12">12</option>
                                                <option value="13">13</option>
                                                <option value="14">14</option>
                                                <option value="15">15</option>
                                                <option value="16">16</option>
                                                <option value="17">17</option>
                                                <option value="18">18</option>
                                                <option value="19">19</option>
                                                <option value="20">20</option>
                                                <option value="21">21</option>
                                                <option value="22">22</option>
                                                <option value="23">23</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="input-group">
                                            <select class="form-control input-sm" id="pickupMINReturn" name="pickupMINReturn">
                                                <option value="" selected="selected">Min</option>
                                                <option value="00">00</option>
                                                <option value="05">05</option>
                                                <option value="10">10</option>
                                                <option value="15">15</option>
                                                <option value="20">20</option>
                                                <option value="25">25</option>
                                                <option value="30">30</option>
                                                <option value="35">35</option>
                                                <option value="40">40</option>
                                                <option value="45">45</option>
                                                <option value="50">50</option>
                                                <option value="55">55</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="input-sm">@Html.ActionLink("See Prices", "getPrice", "Price", null, new { target = "_blank" })</div>
                            <div id="mess_divCard" style="display:none;">
                                <label style="color:red; font-size:small;">Bei jede Bezahlung mit Kreditkarte wird + 5 € Kreditkartengebür</label>
                            </div>
                            <div>
                                <label style="color:red; font-size:small;">Bei jede weitere zusatzadresse + 8 €</label>
                            </div>
                            <button type="button" id="submit" value="Reserve" class="btn btn-info btn-block">Reserve <span id="anim" class="glyphicon glyphicon glyphicon-refresh"></span></button>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




<script src="~/Scripts/Application/Booking.js"></script>
